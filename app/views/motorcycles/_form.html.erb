<%= form_for [@brand, @motorcycle] do |f| %>
  <div>
    <%= f.label(:type) %>
    <%= f.text_field(:type) %>
  </div>

  <div>
    <%= f.label(:category) %>
    <%= f.text_field(:category) %>
  </div>

  <div>
    <%= f.label(:production_since) %>
    <%= f.number_field(:production_since) %>
  </div>

  <div>
    <%= f.label(:photo) %>
    <%= f.file_field(:photo) %>
  </div>

  <div>
    <%= f.submit "Add model" %>
  </div>
<% end %>
